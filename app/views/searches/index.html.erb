<h1>Searches#index</h1>
<p>Find me in app/views/searches/index.html.erb</p>

<%= form_with url: searches_path, method: :get do |f| %>
  <%= f.text_field :keyword, placeholder: "検索ワードを入力" %>
  <%= f.submit "検索" %>
<% end %>

<% if @tiktok_url.present? %>
  <p>生成されたURL: <%= @tiktok_url %></p>
<% end %>

